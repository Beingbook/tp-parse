{
  "private": true,
  "scripts": {
    "test": "mocha tests/* --require babel-core/register --require babel-polyfill --require ./tests/helper --recursive",
    "start": "babel-node tools/run dev",
    "build": "npm run test && babel-node tools/run build -- --release",
    "lint": "eslint . --ext .js,.jsx",
    "clean": "babel-node tools/run clean",
    "deploy": "npm run lint && npm run test && babel-node tools/run deploy -- --release"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  },
  "engines": {
    "node": ">=5.0 <6",
    "npm": ">=3.3 <4"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "bluebird": "^3.4.1",
    "express": "^4.14.0",
    "parse-server": "^2.2.16",
    "source-map-support": "^0.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.9.0",
    "browser-sync": "^2.13.0",
    "chai": "^3.5.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb-base": "^4.0.0",
    "eslint-plugin-import": "^1.10.2",
    "file-loader": "^0.9.0",
    "gaze": "^1.1.0",
    "git-repository": "^0.1.4",
    "json-loader": "^0.5.4",
    "lodash": "^4.13.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.5.3",
    "ncp": "^2.0.0",
    "raw-loader": "^0.5.1",
    "replace": "^0.3.0",
    "supertest": "^1.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  }
}
