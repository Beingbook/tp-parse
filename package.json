{
  "private": true,
  "scripts": {
    "test": "env NODE_PATH=$NODE_PATH:$PWD/src mocha tests/* --require babel-core/register --require babel-polyfill --require ./tests/helper --recursive",
    "start": "$(npm bin)/babel-node tools/run dev",
    "build": "npm run test && $(npm bin)/babel-node tools/run build -- --release",
    "lint": "$(npm bin)/eslint . --ext .js,.jsx",
    "clean": "$(npm bin)/babel-node tools/run clean",
    "deploy": "npm run lint && npm run test && $(npm bin)/babel-node tools/run deploy -- --release"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "stage-1",
      "stage-2",
      "stage-3"
    ]
  },
  "engines": {
    "node": ">=5.0 <6",
    "npm": ">=3.3 <4"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "bcrypt": "^0.8.5",
    "bluebird": "^3.3.3",
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "jade": "^1.11.0",
    "kerberos": "0.0.18",
    "mongoose": "^4.4.6",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "shortid": "^2.2.4",
    "source-map-support": "^0.4.0",
    "validator": "^5.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-register": "^6.6.5",
    "bluebird": "^3.3.3",
    "browser-sync": "^2.11.1",
    "chai": "^3.5.0",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.0.2",
    "file-loader": "^0.8.5",
    "gaze": "^0.5.2",
    "git-repository": "^0.1.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.6.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "ncp": "^2.0.0",
    "raw-loader": "^0.5.1",
    "replace": "^0.3.0",
    "supertest": "^1.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14"
  }
}
